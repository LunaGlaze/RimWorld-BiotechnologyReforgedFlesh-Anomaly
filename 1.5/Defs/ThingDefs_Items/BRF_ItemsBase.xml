      <soundImpactMelee>Pawn_Melee_Punch_HitBuilding_Metal</soundImpactMelee>
      <statOffsets>
        <Beauty>0</Beauty>
      </statOffsets>
      <statFactors>
        <MaxHitPoints>2.5</MaxHitPoints>
