<?xml version="1.0" encoding="utf-8" ?>
<Defs>

	<ThingDef Name="BRF_FreshShellBodyPartBase" ParentName="BodyPartBase" Abstract="True">
		<graphicData>
			<texPath>Things/Item/Health/HealthItem</texPath>
			<graphicClass>Graphic_Single</graphicClass>
			<drawSize>0.80</drawSize>
			<color>(200,100,100)</color>
		</graphicData>
	  <tickerType>Rare</tickerType>
    <thingCategories>
      <li>BRF_Things_FreshShell_Base</li>
    </thingCategories>
    <statBases>
      <DeteriorationRate>4.0</DeteriorationRate>
    </statBases>
	  <comps>
      <li Class="CompProperties_Rottable">
        <daysToRotStart>3</daysToRotStart>
        <rotDestroys>true</rotDestroys>
      </li>
    </comps>
  </ThingDef>

	<ThingDef Name="BRF_FreshShellBaseBodyPart" ParentName="BRF_FreshShellBodyPartBase" Abstract="True">
    <recipeMaker>
      <workSpeedStat>GeneralLaborSpeed</workSpeedStat>
      <workSkill>Crafting</workSkill>
      <effectWorking>Smith</effectWorking>
      <soundWorking>Recipe_CookMeal</soundWorking>
      <recipeUsers>
        <li>BRF_BioForge</li>
      </recipeUsers>
      <skillRequirements>
        <Crafting>5</Crafting>
      </skillRequirements>
      <unfinishedThingDef>UnfinishedHealthItemProsthetic</unfinishedThingDef>
      <researchPrerequisite>BRF_FleshShellBaseResearch</researchPrerequisite>
    </recipeMaker>
  </ThingDef>
  
  <RecipeDef Name="BRF_SurgeryInstallBodyPartNaturalBase" ParentName="SurgeryInstallImplantBase" Abstract="True">
    <workerClass>Recipe_InstallArtificialBodyPart</workerClass>
  </RecipeDef>

<!-- 手臂 -->
  <ThingDef ParentName="BRF_FreshShellBaseBodyPart">
    <defName>BRF_FleshMadeArm</defName>
    <label>flesh-made arm</label>
    <description>A remodeled arm, constructed using basic flesh materials. Using initially remodeled flesh and blood as a replacement for the native shell, it doesn't work any better, but we're well on our way to ascension.</description>
    <descriptionHyperlinks><RecipeDef>BRF_InstallFleshBaseArm</RecipeDef></descriptionHyperlinks>
    <statBases>
      <WorkToMake>5000</WorkToMake>
      <MarketValue>45</MarketValue>
      <Mass>1</Mass>
    </statBases>
	  <tradeability>None</tradeability>
    <costList>
      <BRF_ReforgedMeat_Human>10</BRF_ReforgedMeat_Human>
      <BRF_BoneFragment>3</BRF_BoneFragment>
      <BRF_LivingFlesh>1</BRF_LivingFlesh>
      <BRF_FleshMadeHand>1</BRF_FleshMadeHand>
    </costList>
  </ThingDef>

  <RecipeDef ParentName="BRF_SurgeryInstallBodyPartNaturalBase">
    <defName>BRF_InstallFleshBaseArm</defName>
    <label>install flesh-made arm</label>
    <description>Install a flesh-made arm. Using initially remodeled flesh and blood as a replacement for the native shell, it doesn't work any better, but we're well on our way to ascension.</description>
    <jobString>Installing flesh-made arm.</jobString>
    <ingredients>
      <li>
        <filter>
          <thingDefs>
            <li>BRF_FleshMadeArm</li>
          </thingDefs>
        </filter>
        <count>1</count>
      </li>
    </ingredients>
    <fixedIngredientFilter>
      <thingDefs>
        <li>BRF_FleshMadeArm</li>
      </thingDefs>
    </fixedIngredientFilter>
    <appliedOnFixedBodyParts>
      <li>Arm</li>
    </appliedOnFixedBodyParts>
	  <addsHediff>BRF_FleshMadeArmHediff</addsHediff>
  </RecipeDef>
  
  <HediffDef ParentName="AddedBodyPartBase">
    <defName>BRF_FleshMadeArmHediff</defName>
    <label>flesh-made arm</label>
    <labelNoun>a flesh-made arm</labelNoun>
    <description>An installed flesh-made arm.</description>
    <descriptionHyperlinks><ThingDef>BRF_FleshMadeArm</ThingDef></descriptionHyperlinks>
    <comps>
      <li Class="HediffCompProperties_VerbGiver">
        <tools>
          <li>
            <label>fist</label>
            <capacities>
              <li>Blunt</li>
            </capacities>
            <power>8.2</power> <!-- Same as natural fist -->
            <cooldownTime>2</cooldownTime>
          </li>
        </tools>
      </li>
    </comps>
    <spawnThingOnRemoved>BRF_FleshMadeArm</spawnThingOnRemoved>
    <addedPartProps>
      <partEfficiency>1</partEfficiency>
    </addedPartProps>
  </HediffDef>
  
