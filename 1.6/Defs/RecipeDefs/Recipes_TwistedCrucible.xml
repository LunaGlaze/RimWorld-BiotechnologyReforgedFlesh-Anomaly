<!--血肉立方-->
  <RecipeDef>
    <defName>BRF_ShapingFleshCubes</defName>
    <label>shaping recast flesh cube</label>
    <description>synthesize reforged meats and plants into some sort of bizarre cube-mounted object.</description>
    <jobString>Shaping recast flesh cube.</jobString>
    <workAmount>600</workAmount>
    <workSpeedStat>CookSpeed</workSpeedStat>
    <effectWorking>Cook</effectWorking>
    <soundWorking>Recipe_CookMeal</soundWorking>
    <workSkill>Cooking</workSkill>
    <recipeUsers>
      <li>BRF_TwistedCrucible</li>
    </recipeUsers>
    <ingredients>
      <li>
        <filter>
          <categories>
