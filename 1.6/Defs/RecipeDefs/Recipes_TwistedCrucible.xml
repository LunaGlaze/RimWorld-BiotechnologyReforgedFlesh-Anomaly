          </categories>
        </filter>
        <count>10</count>
      </li>
      <li>
        <filter>
          <categories>
            <li>PlantFoodRaw</li>
          </categories>
        </filter>
        <count>10</count>
      </li>
      <li>
        <filter>
          <thingDefs>
            <li>WoodLog</li>
          </thingDefs>
        </filter>
        <count>5</count>
      </li>
    </ingredients>
    <fixedIngredientFilter>
      <categories>
        <li>BRF_Things_ReforgedMeat</li>
        <li>PlantFoodRaw</li>
      </categories>
      <thingDefs>
        <li>WoodLog</li>
      </thingDefs>
      <disallowedThingDefs>
        <li>BRF_LivingFlesh</li>
        <li>BRF_ReforgedMeat_Harbinger</li>
      </disallowedThingDefs>
    </fixedIngredientFilter>
    <products>
      <BRF_RecastFleshCube>10</BRF_RecastFleshCube>
    </products>
    <researchPrerequisite>BRF_CrimsonFurnitureResearch</researchPrerequisite>
  </RecipeDef>
