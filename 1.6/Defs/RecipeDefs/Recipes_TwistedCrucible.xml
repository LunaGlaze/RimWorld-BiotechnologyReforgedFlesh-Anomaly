<!--制造扭曲肉-->
  <RecipeDef>
    <defName>BRF_ShapingTwistedMeat</defName>
    <label>Reshape Twisted Meat</label>
    <description>Breeding Twisted Meat with Reforged Meat and Bioferrite.</description>
    <jobString>Reshaping Twisted Meat</jobString>
    <workAmount>800</workAmount>
    <workSpeedStat>CookSpeed</workSpeedStat>
    <effectWorking>Cook</effectWorking>
    <soundWorking>Recipe_CookMeal</soundWorking>
    <workSkill>Cooking</workSkill>
    <recipeUsers>
      <li>BRF_TwistedCrucible</li>
    </recipeUsers>
    <ingredients>
      <li>
        <filter>
          <categories>
            <li>BRF_Things_ReforgedMeat</li>
          </categories>
        </filter>
        <count>30</count>
      </li>
      <li>
        <filter>
          <thingDefs>
            <li>Bioferrite</li>
          </thingDefs>
        </filter>
        <count>1</count>
      </li>
    </ingredients>
    <fixedIngredientFilter>
      <categories>
        <li>BRF_Things_ReforgedMeat</li>
      </categories>
    </fixedIngredientFilter>
    <products>
      <Meat_Twisted>30</Meat_Twisted>
    </products>
    <researchPrerequisite>BRF_ShapingTwistedMeatResearch</researchPrerequisite>
  </RecipeDef>

