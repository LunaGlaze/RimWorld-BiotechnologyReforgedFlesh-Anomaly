  <RecipeDef>
    <defName>BRF_ReforgedMeat_Human1</defName>
    <label>reconstruct a little human flesh</label>
    <description>reconstruct human flesh into something may edible.</description>
    <jobString>Reconstructing human flesh.</jobString>
    <workAmount>200</workAmount>
    <workSpeedStat>CookSpeed</workSpeedStat>
    <effectWorking>Cook</effectWorking>
    <soundWorking>Recipe_CookMeal</soundWorking>
    <workSkill>Cooking</workSkill>
    <recipeUsers>
      <li>BRF_TwistedCrucible</li>
    </recipeUsers>
    <ingredients>
      <li>
        <filter>
          <thingDefs>
            <li>Meat_Human</li>
          </thingDefs>
        </filter>
        <count>10</count>
      </li>
    </ingredients>
    <products>
      <BRF_ReforgedMeat_Human>7</BRF_ReforgedMeat_Human>
    </products>
    <researchPrerequisite>BRF_TwistedCrucibleResearch</researchPrerequisite>
  </RecipeDef>
