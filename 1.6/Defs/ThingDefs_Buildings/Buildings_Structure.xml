    </statBases>
    <affordances>
      <li>Light</li>
      <li>Medium</li>
      <li>Heavy</li>
	  <li>BRF_FleshBlanket</li>
    </affordances>
	<fertility>0.05</fertility>
	<designationCategory IsNull="True" />
  </TerrainDef>
