  <ThinkTreeDef>
    <defName>BRF_ScarletCerebralJellyfishThinkTree</defName>
    <thinkRoot Class="ThinkNode_Priority">
      <subNodes>
        <!-- Despawned -->
        <li Class="ThinkNode_Subtree">
          <treeDef>Despawned</treeDef>
        </li>
        <!-- Downed -->
