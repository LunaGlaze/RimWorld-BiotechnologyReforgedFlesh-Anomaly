<!--物种ThingsDef-->
  <ThingDef ParentName="BRF_PrimordialFleshBeast">
    <defName>BRF_PrimordialFleshBeast</defName>
    <label>Primordial Fleshbeast</label>
    <description>A strange and deep red fleshbeast, originally found amongst pit gates. They are shaped like fingerspikes, but have powerful tentacles instead of spikes.</description>
    <statBases>
      <MoveSpeed>5.5</MoveSpeed>
      <MinimumContainmentStrength>50</MinimumContainmentStrength>
      <ToxicEnvironmentResistance>1</ToxicEnvironmentResistance>
      <ToxicResistance>1</ToxicResistance>
      <InjuryHealingFactor>5</InjuryHealingFactor>
      <PainShockThreshold>0.99</PainShockThreshold>
    </statBases>
    <uiIconPath>Pawns/BRF_PrimordialFleshBeast/BRF_PrimordialFleshBeast_MenuIcon</uiIconPath>
    <race>
      <body>BRF_PrimordialFleshBeast</body>
      <baseBodySize>0.6</baseBodySize>
      <baseHealthScale>2.5</baseHealthScale>
      <lifeExpectancy>1000</lifeExpectancy>
      <renderTree>BRF_PrimordialFleshBeast</renderTree>
      <deathAction Class="DeathActionProperties_Vanish">
        <workerClass>DeathActionWorker_Vanish</workerClass>
        <filth>Filth_BRF_PrimordialTwistedFlesh</filth>
        <filthCountRange>1~3</filthCountRange>
        <meatExplosionSize>Small</meatExplosionSize>
      </deathAction>
      <lifeStageAges>
        <li>
          <def>EntityFullyFormed</def>
          <soundWounded>Pawn_Fleshbeast_Fingerspike_Wounded</soundWounded>
          <soundDeath>Pawn_Fleshbeast_Fingerspike_Death</soundDeath>
          <soundCall>Pawn_Fleshbeast_Fingerspike_Call</soundCall>
        </li>
      </lifeStageAges>
    </race>
    <comps>
      <li Class="CompProperties_AttachPoints">
        <points>
          <li>
            <offset>(-.21, 0, .21)</offset>
            <type>PlatformRestraint0</type>
          </li>
          <li>
            <offset>(.21, 0, .21)</offset>
            <type>PlatformRestraint1</type>
          </li>
          <li>
            <offset>(.21, 0, -.21)</offset>
            <type>PlatformRestraint2</type>
          </li>
          <li>
            <offset>(-.21, 0, -.21)</offset>
            <type>PlatformRestraint3</type>
          </li>
        </points>
      </li>
      <li Class="CompProperties_CanBeDormant">
        <maxDistAwakenByOther>3</maxDistAwakenByOther>
        <wakeUpDelayRange>100~200</wakeUpDelayRange>
        <jobDormancy>true</jobDormancy>
      </li>
      <li Class="CompProperties_Studiable">
        <frequencyTicks>120000</frequencyTicks>
        <knowledgeCategory>Advanced</knowledgeCategory>
        <minMonolithLevelForStudy>1</minMonolithLevelForStudy>
        <anomalyKnowledge>1</anomalyKnowledge>
        <requiresHoldingPlatform>true</requiresHoldingPlatform>
      </li>
      <li Class="CompProperties_ProducesBioferrite">
        <bioferriteDensity>3</bioferriteDensity>
      </li>
    </comps>
    <tools>
      <li>
        <label>great tentacle</label>
        <labelNoLocation>great tentacle</labelNoLocation>
        <capacities>
          <li>Blunt</li>
        </capacities>
        <power>18</power>
        <cooldownTime>1.5</cooldownTime>
        <armorPenetration>0.3</armorPenetration>
        <linkedBodyPartsGroup>MiddleSpike</linkedBodyPartsGroup>
        <alwaysTreatAsWeapon>true</alwaysTreatAsWeapon>
        <chanceFactor>0.5</chanceFactor>
        <soundMeleeHit>Pawn_Fleshbeast_Attack_Blunt</soundMeleeHit>
      </li>
      <li>
        <label>head</label>
        <capacities>
          <li>Blunt</li>
        </capacities>
        <power>10</power>
        <cooldownTime>1.5</cooldownTime>
        <armorPenetration>0.3</armorPenetration>
        <linkedBodyPartsGroup>HeadAttackTool</linkedBodyPartsGroup>
        <ensureLinkedBodyPartsGroupAlwaysUsable>true</ensureLinkedBodyPartsGroupAlwaysUsable>
        <chanceFactor>0.2</chanceFactor>
        <soundMeleeHit>Pawn_Fleshbeast_Attack_Blunt</soundMeleeHit>
      </li>
    </tools>
    <killedLeavings>
      <BRF_PrimordialTissueSample>1</BRF_PrimordialTissueSample>
    </killedLeavings>
  </ThingDef>
</Defs>