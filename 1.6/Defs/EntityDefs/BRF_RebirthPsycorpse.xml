<!--默认物种Things-->
  <ThingDef ParentName="BaseFleshbeast" Name="BRF_RebirthPsycorpse" Abstract="True">
    <statBases>
      <ComfyTemperatureMin>-250</ComfyTemperatureMin>
      <ComfyTemperatureMax>250</ComfyTemperatureMax>
      <Flammability>0</Flammability>
