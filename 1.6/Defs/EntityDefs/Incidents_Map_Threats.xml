  <IncidentDef ParentName="AnomalyIncidentBase">
    <defName>BRF_CreepJoiner_RebirthPsycorpse</defName>
    <label>creepjoiner join</label>
    <category>Misc</category>
    <targetTags>
      <li>Map_PlayerHome</li>
    </targetTags>
	  <workerClass>Luna_BRF.IncidentWorker_CreepJoiner_RebirthPsycorpse</workerClass>
    <questScriptDef>BRF_CreepJoinerArrival_RebirthPsycorpse</questScriptDef>
    <earliestDay>20</earliestDay>
    <baseChance>0.1</baseChance>
    <minRefireDays>60</minRefireDays>
    <minAnomalyThreatLevel>2</minAnomalyThreatLevel>
    <requireColonistsPresent>True</requireColonistsPresent>
  </IncidentDef>
  
  <QuestScriptDef ParentName="CreepjoinerArrivalBase">
    <defName>BRF_CreepJoinerArrival_RebirthPsycorpse</defName>
    <root Class="QuestNode_Sequence">
      <nodes>
        <li Class="QuestNode_SetupCreepjoiner">
          <downside>BRF_CreepJoiner_RebirthPsycorpse</downside>
          <aggressive>Assault</aggressive>
          <rejection>AggressiveRejection</rejection>
          <storeFormAs>form</storeFormAs>
          <storeBenefitAs>benefit</storeBenefitAs>
          <storeDownsideAs>downside</storeDownsideAs>
          <storeAggressiveAs>aggressive</storeAggressiveAs>
          <storeRejectionAs>rejection</storeRejectionAs>
        </li>
        <li Class="QuestNode_Root_Creepjoiner_Arrival" />
      </nodes>
    </root>
  </QuestScriptDef>
  
